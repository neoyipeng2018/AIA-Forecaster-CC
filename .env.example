# LLM Configuration
# Model string follows litellm format: "provider/model-name"
# Examples:
#   anthropic/claude-sonnet-4-5-20250929
#   openai/o3
#   deepseek/deepseek-chat
LLM_MODEL=anthropic/claude-sonnet-4-5-20250929
ANTHROPIC_API_KEY=
OPENAI_API_KEY=
DEEPSEEK_API_KEY=

# Forecaster Settings
NUM_AGENTS=10
MAX_SEARCH_ITERATIONS=5
PLATT_ALPHA=1.7320508075688772

# Default Currency Pair
DEFAULT_PAIR=USDJPY

# Storage
CACHE_DIR=data/cache
DB_PATH=data/forecasts/forecasts.db
CACHE_TTL_HOURS=6
